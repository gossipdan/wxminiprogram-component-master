page{
  width: 100%;
  height: 100%;
  background: #212121;
}
.video_container{
  position: relative;
  background: #000;
}
.video_play{
  width: 100%;
  height: 100%;
  transform-origin: 50% 50%;
}
.mask_img{
  width: 540rpx;
  height: 540rpx;
  position: absolute;
}
.mask_view{
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 100;
}
.select_container{
  position: relative;
}
.duration_time_view{
  position: absolute;
  font-size: 13px;
  color: #fff;
  text-align: right;
  top: -40rpx;
  right: 20rpx;
}
.move_area{
  height: 146rpx;
  width: 575rpx;
  margin: 86rpx auto;
  margin-bottom: 0;
  position: relative;
}
.progress_bar_icon{
  width: 2rpx;
  height: 100%;
  position: absolute;
  top: 0;
  background: #fff;
  z-index: 2;
}
.progress_grey_background{
  width: 100%;
  height: 100%;
  background: grey;
  position: absolute;
  top: 0;
  left: 0;
}
.progress_background_view{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  background: rgba(245, 90, 69, 0.95);
}
.move_slide_line{
  width: 40rpx;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 3;
}

.slide_img{
  position: absolute;
  width: 52rpx;
  height: 100%;
  display: block;
  left: -10rpx;
  z-index: 1;
}
.number_progress_tipe{
  position: absolute;
  display: block;
  width: 100rpx;
  height: 54rpx;
  color: #fff;
  font-size: 26rpx;
  line-height: 54rpx;
  text-align: center;
  left: -40rpx;
  top: -84rpx;
  background-size: 100%;
}
.text_progress_tipe {
  position: absolute;
  width: 100rpx;
  height: 54rpx;
  color: #fff;
  font-size: 26rpx;
  line-height: 54rpx;
  text-align: center;
  left: -40rpx;
  top: -88rpx;
  z-index: 9;
}
.progress_text_info{
  width: 100%;
  text-align: center;
  font-size: 24rpx;
  color: rgba(255, 255, 255, 0.8);
  margin-top: 24rpx;
}

.footer {
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 1000;
  background: #111111;
}
.operate_btn{
  width: 50%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
  font-size: 36rpx;
  color: #666666;
}
.isolation{
  color: #222222;
  font-size: 36rpx;
  font-weight: bold;
}
.confirm{
  color: #FFFFFF;
}